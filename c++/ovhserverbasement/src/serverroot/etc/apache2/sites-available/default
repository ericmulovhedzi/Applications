
# ------ sabfleet.co.za Settings - OVH Server Basement Version 2.0 

<VirtualHost IP=199.255.255.255:80>
        ServerName sabfleet.co.za
        ServerAlias www.sabfleet.co.za
        ServerAdmin webmaster@sabfleet.co.za
        DocumentRoot /Users/ericmulovhedzi/research/c++/ovhserverbasement/serverroot/var/www/sabfleet.co.za/public_html/

        <Directory /Users/ericmulovhedzi/research/c++/ovhserverbasement/serverroot/var/www/sabfleet.co.za/public_html/>
                Options Indexes FollowSymLinks MultiViews +Includes
                AllowOverride None
                Order allow,deny
                Allow from all
        </Directory>
</VirtualHost>

